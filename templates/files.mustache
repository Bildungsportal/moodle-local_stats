<div class="local_stats files">
    <table class="generaltable">
        <thead>
            <tr>
                <td> {{#str}} file {{/str}}</td>
            </tr>
        </thead>
        <tbody>
        {{#filerecords}}
            <tr>
                <td>{{filepath}}</td>
                <td>
                    {{#files}}
                    <a href="{{{url}}}" class="btn btn-link" target="_blank">
                        {{filename}}
                    </a>
                    {{/files}}
                </td>
            </tr>
        {{/filerecords}}
        </tbody>
    </table>
</div>
